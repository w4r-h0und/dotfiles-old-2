alias python=python3

# Zsh customizations
alias ea="nano $HOME/.oh-my-zsh/custom/aliases.zsh"
alias ef="nano $HOME/.oh-my-zsh/custom/functions.zsh"
alias ez="nano $HOME/.zshrc"
alias sz="source $HOME/.zshrc"

# Running binaries under easier names
alias neofetch="fastfetch"
alias vim="nvim"
alias top="btop"

